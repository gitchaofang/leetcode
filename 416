class Solution {
public:
    bool canPartition(vector<int>& nums) {
        int tot = std::accumulate(nums.begin(), nums.end(),0);
        if(tot % 2 != 0) return false;
        int targ = tot / 2;
        std::vector<bool> dp(targ + 1, false);
        dp[0] = true;
        for(const int& num: nums){
            for(int i = targ; i >= num; --i) dp[i] = dp[i] || dp[i - num];
        }
        return dp[targ];
    }
};
