class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        int n = nums.size();
        if(n == 0) return {};
        std::vector<int> res;
        for(int i=0; i<n; ++i){
            nums[(nums[i]-1)%(n+1)] += (n+1);
        }
        for(int i=0; i<n; ++i){
            if(nums[i]/(n+1) == 2) res.push_back(i+1);
        }
        return res;
    }
};
