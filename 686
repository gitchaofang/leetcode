class Solution {
public:
    int repeatedStringMatch(string a, string b) {
        int n = a.size();
        int m = b.size();
        for(int i = 0; i < n; ++i){
            int j = 0;
            while(j < m && b[j] == a[(i + j) % n]) ++j;
            if(j != m) continue;
            return (i + j - 1) / n + 1;
        }
        return -1;
    }
};
