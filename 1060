class Solution {
public:
    int missingElement(vector<int>& nums, int k) {
        int miss_num = 0;
        for(int i = 1; i<nums.size(); ++i){
            miss_num += nums[i] - nums[i - 1] - 1;
            if(miss_num >= k) return nums[i] - miss_num + k - 1;
        }
        return nums.back() + k - miss_num;
    }
};
