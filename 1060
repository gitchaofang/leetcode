class Solution {
public:
    int missingElement(vector<int>& nums, int k) {
        for(int i = 1; i < nums.size(); ++i){
            if(nums[i] == nums[i - 1]) continue;
            if(nums[i] - nums[i - 1] - 1 >= k) return nums[i - 1] + k;
            else k -= (nums[i] - nums[i - 1] - 1);
        }
        return nums.back() + k;
        
    }
};
