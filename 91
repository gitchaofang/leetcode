class Solution {
public:
    int numDecodings(string s) {
        int n = s.size();
        std::vector<int> dp(n + 1,1);
        for(int i=n-1; i>=0; --i){
            if(s[i] == '0'){ 
                dp[i] = 0;
                continue;
            }
            dp[i] = dp[i + 1];
            if(i < n-1 && std::stoi(s.substr(i,2)) < 27) dp[i] += dp[i+2];
        }
        return dp[0];
    }
};
